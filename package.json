{
  "name": "hotdog-diaries",
  "version": "1.0.0",
  "description": "A website that scans social media for hotdog content",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:prod": "npm run cleanup && next build",
    "start": "next start",
    "start:test": "next start --port 3000",
    "lint": "next lint --max-warnings=800",
    "lint:ci": "next lint --max-warnings=800",
    "type-check": "tsc --noEmit || true",
    "type-check:strict": "tsc --noEmit",
    "type-check:count": "tsc --noEmit 2>&1 | grep -c 'error TS' || echo 0",
    "type-check:unused": "tsc --noEmit 2>&1 | grep 'TS6133' || echo 'No unused variable errors'",
    "type-check:unknown": "tsc --noEmit 2>&1 | grep 'TS18046' || echo 'No unknown type errors'",
    "cleanup": "node scripts/cleanup-prod.js",
    "analyze": "ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "db:migrate": "tsx scripts/migrate-database.ts",
    "db:migrate:supabase": "tsx scripts/migrate-supabase.ts",
    "db:migrate:postgres": "tsx scripts/migrate-to-postgres.ts",
    "db:rollback": "tsx lib/migrations/migrator.ts rollback",
    "db:status": "tsx lib/migrations/migrator.ts status",
    "db:init": "tsx scripts/init-sqlite.ts",
    "db:setup-vercel": "tsx scripts/setup-vercel-postgres.ts",
    "admin:create": "tsx scripts/seed-admin.ts",
    "admin:create-prod": "NODE_ENV=production tsx scripts/create-prod-admin.ts",
    "generate:service-token": "tsx scripts/generate-service-token.ts",
    "db:audit": "tsx scripts/db-audit.ts",
    "admin:debug": "curl -s http://localhost:3000/api/admin/debug | jq .",
    "postbuild": "echo 'Skipping Vercel DB setup during build'"
  },
  "dependencies": {
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.14",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.8",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^10.0.0",
    "@vercel/postgres": "^0.10.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.534.0",
    "next": "^15.4.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "playwright": "^1.54.1",
    "playwright-extra": "^4.3.6",
    "proxy-agent": "^6.5.0",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "snoowrap": "^1.23.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "user-agents": "^1.1.617",
    "utf-8-validate": "^6.0.5",
    "uuid": "^11.1.0",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/react-dom": "^19.1.7",
    "eslint": "^9.31.0",
    "eslint-config-next": "^15.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "tsx": "^4.20.3"
  }
}
